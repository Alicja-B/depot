<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<% cache ['store', Product.latest] do %>
<div class="container-fluid">
  <div class="row">
  <% @products.each do |product| %>
    <% cache ['entry', product] do %>
    <div class="entry">
      <%= render(partial: "shared/product", object: product) %>
    </div>
  <% end %>
<% end %>
</div>
<% end %>
